              <input id ="searchinput" type="text" class="form-control border-secondary text-white bg-transparent" placeholder="Search for services" aria-label="Enter Search" aria-describedby="button-addon2">
              <div class="input-group-append">
                <button  class="btn btn-primary" type="submit" id="button-addon2">Search</button>
              </div>
            </div>
          </form>
        </div>

        <!--  -->
      </div>


    </div>
  </div>


  <div id="spdiv" class="site-section bg-light">

    <div class="container">
      <div class="row align-items-left justify-content-left">
